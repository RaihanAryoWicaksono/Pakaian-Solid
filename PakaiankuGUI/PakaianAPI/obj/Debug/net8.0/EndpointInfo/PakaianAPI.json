            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "AksiPakaian": {
        "enum": [
          "TambahKeKeranjang",
          "KeluarkanDariKeranjang",
          "Pesan",
          "Bayar",
          "Kirim",
          "Selesai",
          "Retur",
          "Batalkan",
          "StokHabis",
          "RestokPakaian",
          "TerimaPakaian",
          "SelesaiCheckout"
        ],
        "type": "string"
      },
      "CheckoutDto": {
        "required": [
          "alamatPengiriman",
          "metodePembayaran"
        ],
        "type": "object",
        "properties": {
          "alamatPengiriman": {
            "minLength": 1,
            "type": "string"
          },
          "metodePembayaran": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CheckoutResponseDto": {
        "type": "object",
        "properties": {
          "orderId": {
            "type": "string",
            "nullable": true
          },
          "tanggalPemesanan": {
            "type": "string",
            "format": "date-time"
          },
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PakaianDto"
            },
            "nullable": true
          },
          "totalHarga": {
            "type": "number",
            "format": "double"
          },
          "statusPemesanan": {
            "type": "string",
            "nullable": true
          },
          "alamatPengiriman": {
            "type": "string",
            "nullable": true
          },
          "metodePembayaran": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreatePakaianDto": {
        "required": [
          "harga",
          "kategori",
          "kode",
          "nama",
          "stok",
          "ukuran",
          "warna"
        ],
        "type": "object",
        "properties": {
          "kode": {
            "minLength": 1,
            "type": "string"
          },
          "nama": {
            "minLength": 1,
            "type": "string"
          },
          "kategori": {
            "minLength": 1,
            "type": "string"
          },
          "warna": {
            "minLength": 1,
            "type": "string"
          },
          "ukuran": {
            "minLength": 1,
            "type": "string"
          },
          "harga": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "stok": {
            "maximum": 2147483647,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "KeranjangDto": {
        "type": "object",
        "properties": {
          "items": {
            "type": "array",
