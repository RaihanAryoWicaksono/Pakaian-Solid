          "kode": {
            "type": "string",
            "nullable": true
          },
          "nama": {
            "type": "string",
            "nullable": true
          },
          "kategori": {
            "type": "string",
            "nullable": true
          },
          "warna": {
            "type": "string",
            "nullable": true
          },
          "ukuran": {
            "type": "string",
            "nullable": true
          },
          "harga": {
            "type": "number",
